{
  "name": "shortener",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env OTEL_SERVICE_NAME=shortener-api OTEL_EXPORTER_JAEGER_ENDPOINT=http://localhost:14268/api/traces ts-node-dev src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "seed": "ts-node prisma/seed.ts",
    "prisma": "npx prisma migrate deploy",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=ERROR vitest run",
    "test:coverage": "cross-env NODE_ENV=test LOG_LEVEL=ERROR vitest run --coverage",
    "test:ui": "cross-env NODE_ENV=test LOG_LEVEL=ERROR vitest --ui",
    "test:each": "ts-node src/shared/tests/test-runner.ts",
    "database": "docker compose up -d",
    "database:down": "docker compose down",
    "database:logs": "docker compose logs -f",
    "database:test": "docker compose -f docker-compose-test.yml up --build",
    "prepare": "if [ \"$VERCEL\" != \"1\" ]; then husky install || true; fi",
    "commit": "git add . && git cz",
    "release": "standard-version",
    "vercel-build": "npx prisma generate && pnpm build"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.prisma": [
      "prisma format"
    ]
  }
}
